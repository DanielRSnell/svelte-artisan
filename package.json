{
	"name": "artisan-framework",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"clean:wp": "rimraf ./src/wordpress",
		"build:digitalocean": "yarn install --production=false && yarn run clean && yarn run build && rm -rf node_modules && yarn install --production --frozen-lockfile",
		"dev": "vite dev",
		"build": "npm run refresh && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"refresh": "node ./helpers/refresh.cjs",
		"build:refresh": "npm run build && npm run refresh",
		"dev:refresh": "npm run build && npm run refresh && npm run dev",
		"prod:refresh": "npm run build && npm run refresh && npm run prod",
		"preview:refresh": "npm run build && npm run refresh && npm run preview"
	},
	"devDependencies": {
		"@floating-ui/dom": "^1.4.3",
		"@skeletonlabs/skeleton": "^1.8.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-cloudflare": "^2.3.1",
		"@sveltejs/adapter-node": "^1.3.0",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.5.0",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.9",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.26.0",
		"flowbite": "^1.7.0",
		"flowbite-typography": "^1.0.3",
		"fs-extra": "^11.1.1",
		"got": "^13.0.0",
		"highlight.js": "^11.8.0",
		"install": "^0.13.0",
		"jsdom": "^22.1.0",
		"npm": "^9.7.2",
		"postcss": "^8.4.24",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"rimraf": "^5.0.1",
		"serve": "^14.2.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"unocss": "^0.53.5",
		"vite": "^4.3.0",
		"website-scraper": "^5.3.1",
		"website-scraper-existing-directory": "^1.0.0"
	},
	"type": "module",
	"dependencies": {
		"alpinejs": "^3.12.3",
		"animejs": "^3.2.1",
		"axios": "^1.4.0",
		"cheerio": "1.0.0-rc.12"
	}
}
